{
    "name": "yaskuriyamuri/php_mytools",
    "description": "Tools fungsi kostum untuk PHP",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "version": "v1.0.3",
    "authors": [
        {
            "name": "Yaskuri Yamuri",
            "email": "yaskuriyamuri@gmail.com",
            "homepage": "https://dukung.online",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6||^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Yaskuriyamuri\\Php_mytools\\": "src/",
            "Yaskuriyamuri\\Php_mytools_test\\": "test/"
        } 
    },
    "config": {
        "apcu-autoloader": true,
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "5.6.40"
        }
    },
    "scripts": {
        "post-update-cmd": "./vendor/bin/phpunit --verbose --configuration phpunit.xml"
    }
}